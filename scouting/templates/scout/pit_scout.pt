<!DOCTYPE html>
<html>
<head>
    <title>Pit Scouting</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<body>
    <a href="${request.route_url('home')}">Back</a>
    <h1>Unscouted Robots</h1>
    <table>
        <tr tal:repeat="robot unscouted_robots">
            <td>
                <a href="${request.route_url('scout_robot', robot_number=robot.robot_number)}">
                    ${robot.robot_number}
                </a>
            </td>
        </tr>
    </table>
    <h1>Scouted Robots</h1>
    <table>
        <tr tal:repeat="robot scouted_robots">
            <td>
                <a href="${request.route_url('scout_robot', robot_number=robot.robot_number)}">
                    ${robot.robot_number}
                </a>
            </td>
        </tr>
    </table>
</body>
</html>
